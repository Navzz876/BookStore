@model List<Company>
<div class="container">
    <div class=" row pt-4 pb-4 justify-content-md-around">
        <div class="col-6">
            <h2> Company List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Company" asp-action="CreateCompany" class="link-primary text-primary">
                <i class="bi bi-bag-plus h6 text-primary">
                   
                </i>
                Add Company
            </a>
        </div>
    </div>
<table class="table table-bordered table-striped">
    <tr>
        <th>
            Company Name
        </th>
        <th>
            City
        </th>
            <th>
                State
            </th>
            <th>
                Country
            </th>
            <th>
                Phone
            </th>
        <th></th>
        <th></th>
       
    </tr>
    </table>
        <table id="companyTable"></table>
</div>

@section Scripts {
    <script src="~/js/company.js"></script>
}
